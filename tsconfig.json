{
    "compilerOptions": {
      "outDir": "dist",
      "sourceMap": false,
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "noEmitHelpers": true,
      "lib": [
        "esnext",
        "dom",
        "dom.iterable"
      ]
    },
  }
  